.slicktabslider #hovertabeslid_tab {
display: flex;
align-items: center;
justify-content: space-between;
margin: 30px 0;
flex-wrap: wrap;
border-bottom: 1px solid rgba(25, 32, 32, 0.1);
}


.slicktabslider .tech_item figure {
width: 91px;
height: 91px;
border: 0px solid #e2e2e2;
padding: 17px;
border-radius: 8px;
margin: 0 auto;
transition: all .3s ease-in-out 0s;
